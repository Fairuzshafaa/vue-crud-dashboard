{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { computed, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Sidebar from \"../components/Sidebar.vue\";\nimport StatCard from \"../components/StatCard.vue\";\nimport UsersTable from \"../components/UsersTable.vue\";\nimport { Search, Bell, GraduationCap, Bookmark, CreditCard, User } from \"lucide-vue-next\";\nexport default {\n  __name: 'Dashboard',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const stats = computed(() => store.getters[\"dashboard/stats\"]);\n    const users = computed(() => store.getters[\"dashboard/users\"]);\n    const searchQuery = ref(\"\");\n    const filteredUsers = computed(() => users.value.filter(u => u.name.toLowerCase().includes(searchQuery.value.toLowerCase())));\n    function onEdit(index, row) {\n      const newName = prompt(\"Edit name\", row.name);\n      if (newName) {\n        store.dispatch(\"dashboard/updateUser\", {\n          index,\n          user: {\n            ...row,\n            name: newName\n          }\n        });\n      }\n    }\n    function onDelete(index) {\n      if (confirm(\"Hapus data ini?\")) store.dispatch(\"dashboard/deleteUser\", index);\n    }\n    function formatMoney(v) {\n      return v.toLocaleString(\"en-IN\");\n    }\n    const __returned__ = {\n      store,\n      stats,\n      users,\n      searchQuery,\n      filteredUsers,\n      onEdit,\n      onDelete,\n      formatMoney,\n      computed,\n      ref,\n      get useStore() {\n        return useStore;\n      },\n      Sidebar,\n      StatCard,\n      UsersTable,\n      get Search() {\n        return Search;\n      },\n      get Bell() {\n        return Bell;\n      },\n      get GraduationCap() {\n        return GraduationCap;\n      },\n      get Bookmark() {\n        return Bookmark;\n      },\n      get CreditCard() {\n        return CreditCard;\n      },\n      get User() {\n        return User;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","ref","useStore","Sidebar","StatCard","UsersTable","Search","Bell","GraduationCap","Bookmark","CreditCard","User","store","stats","getters","users","searchQuery","filteredUsers","value","filter","u","name","toLowerCase","includes","onEdit","index","row","newName","prompt","dispatch","user","onDelete","confirm","formatMoney","v","toLocaleString"],"sources":["C:/Users/Fairuz Shafa' Amani/Downloads/vue-crud-dashboard/vue-crud-dashboard/src/pages/Dashboard.vue"],"sourcesContent":["<script setup>\nimport { computed, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Sidebar from \"../components/Sidebar.vue\";\nimport StatCard from \"../components/StatCard.vue\";\nimport UsersTable from \"../components/UsersTable.vue\";\nimport {\n  Search,\n  Bell,\n  GraduationCap,\n  Bookmark,\n  CreditCard,\n  User,\n} from \"lucide-vue-next\";\n\nconst store = useStore();\nconst stats = computed(() => store.getters[\"dashboard/stats\"]);\nconst users = computed(() => store.getters[\"dashboard/users\"]);\n\nconst searchQuery = ref(\"\");\nconst filteredUsers = computed(() =>\n  users.value.filter((u) =>\n    u.name.toLowerCase().includes(searchQuery.value.toLowerCase())\n  )\n);\n\nfunction onEdit(index, row) {\n  const newName = prompt(\"Edit name\", row.name);\n  if (newName) {\n    store.dispatch(\"dashboard/updateUser\", {\n      index,\n      user: { ...row, name: newName },\n    });\n  }\n}\nfunction onDelete(index) {\n  if (confirm(\"Hapus data ini?\")) store.dispatch(\"dashboard/deleteUser\", index);\n}\nfunction formatMoney(v) {\n  return v.toLocaleString(\"en-IN\");\n}\n</script>\n\n<template>\n  <div class=\"layout\">\n    <Sidebar />\n    <main class=\"content\">\n      <!-- Topbar -->\n      <header class=\"topbar\">\n        <div class=\"search-wrapper\">\n          <Search class=\"icon-search\" />\n          <input class=\"search\" placeholder=\"Search...\" v-model=\"searchQuery\" />\n        </div>\n        <Bell class=\"icon-bell\" />\n      </header>\n\n      <!-- Stats -->\n      <section class=\"stats grid\">\n        <StatCard\n          :icon=\"GraduationCap\"\n          title=\"Students\"\n          :value=\"stats.students\"\n          bg=\"var(--card-students)\"\n        />\n        <StatCard\n          :icon=\"Bookmark\"\n          title=\"Course\"\n          :value=\"stats.courses\"\n          bg=\"var(--card-courses)\"\n        />\n        <StatCard\n          :icon=\"CreditCard\"\n          :title=\"`Payments`\"\n          :value=\"`${stats.paymentsLabel} ${formatMoney(stats.payments)}`\"\n          bg=\"var(--card-payments)\"\n        />\n        <StatCard\n          :icon=\"User\"\n          title=\"Users\"\n          :value=\"stats.users\"\n          :gradient=\"true\"\n        />\n      </section>\n\n      <!-- Table -->\n      <UsersTable :rows=\"filteredUsers\" @edit=\"onEdit\" @delete=\"onDelete\" />\n    </main>\n  </div>\n</template>\n\n<style scoped>\n.layout {\n  display: grid;\n  grid-template-columns: 260px 1fr;\n  min-height: 100vh;\n  background: #fafafa;\n}\n.content {\n  padding: var(--space-4);\n}\n\n/* Topbar */\n.topbar {\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n  margin-bottom: var(--space-4);\n  gap: 16px;\n}\n.search-wrapper {\n  position: relative;\n  width: 280px;\n}\n.search {\n  width: 100%;\n  height: 40px;\n  border-radius: 20px;\n  border: 1px solid var(--border);\n  padding: 0 40px 0 14px;\n}\n.icon-search {\n  position: absolute;\n  right: 12px;\n  top: 50%;\n  transform: translateY(-50%);\n  width: 18px;\n  height: 18px;\n  color: var(--muted);\n}\n.icon-bell {\n  width: 22px;\n  height: 22px;\n  color: var(--muted);\n  cursor: pointer;\n}\n\n/* Stats grid */\n.stats {\n  grid-template-columns: repeat(4, 1fr);\n  margin-bottom: var(--space-4);\n}\n@media (max-width: 1100px) {\n  .stats {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n@media (max-width: 640px) {\n  .layout {\n    grid-template-columns: 1fr;\n  }\n}\n</style>\n"],"mappings":";;AACA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,OAAO,MAAM,2BAA2B;AAC/C,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,OAAOC,UAAU,MAAM,8BAA8B;AACrD,SACEC,MAAM,EACNC,IAAI,EACJC,aAAa,EACbC,QAAQ,EACRC,UAAU,EACVC,IAAI,QACC,iBAAiB;;;;;;;IAExB,MAAMC,KAAK,GAAGV,QAAQ,CAAC,CAAC;IACxB,MAAMW,KAAK,GAAGb,QAAQ,CAAC,MAAMY,KAAK,CAACE,OAAO,CAAC,iBAAiB,CAAC,CAAC;IAC9D,MAAMC,KAAK,GAAGf,QAAQ,CAAC,MAAMY,KAAK,CAACE,OAAO,CAAC,iBAAiB,CAAC,CAAC;IAE9D,MAAME,WAAW,GAAGf,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMgB,aAAa,GAAGjB,QAAQ,CAAC,MAC7Be,KAAK,CAACG,KAAK,CAACC,MAAM,CAAEC,CAAC,IACnBA,CAAC,CAACC,IAAI,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACP,WAAW,CAACE,KAAK,CAACI,WAAW,CAAC,CAAC,CAC/D,CACF,CAAC;IAED,SAASE,MAAMA,CAACC,KAAK,EAAEC,GAAG,EAAE;MAC1B,MAAMC,OAAO,GAAGC,MAAM,CAAC,WAAW,EAAEF,GAAG,CAACL,IAAI,CAAC;MAC7C,IAAIM,OAAO,EAAE;QACXf,KAAK,CAACiB,QAAQ,CAAC,sBAAsB,EAAE;UACrCJ,KAAK;UACLK,IAAI,EAAE;YAAE,GAAGJ,GAAG;YAAEL,IAAI,EAAEM;UAAQ;QAChC,CAAC,CAAC;MACJ;IACF;IACA,SAASI,QAAQA,CAACN,KAAK,EAAE;MACvB,IAAIO,OAAO,CAAC,iBAAiB,CAAC,EAAEpB,KAAK,CAACiB,QAAQ,CAAC,sBAAsB,EAAEJ,KAAK,CAAC;IAC/E;IACA,SAASQ,WAAWA,CAACC,CAAC,EAAE;MACtB,OAAOA,CAAC,CAACC,cAAc,CAAC,OAAO,CAAC;IAClC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}