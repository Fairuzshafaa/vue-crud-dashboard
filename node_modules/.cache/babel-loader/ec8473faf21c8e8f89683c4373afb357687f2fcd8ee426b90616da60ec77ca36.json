n}\n\nfunction onDelete(index) {\n  if (confirm(\"Hapus data ini?\")) {\n    store.dispatch(\"dashboard/deleteUser\", index);\n  }\n}\n\nfunction formatMoney(v) {\n  return v.toLocaleString(\"en-IN\");\n}\n</script>\n\n<template>\n  <div class=\"layout\">\n    <Sidebar />\n\n    <main class=\"content\">\n      <!-- Topbar -->\n      <header class=\"topbar\">\n        <div class=\"search-wrapper\">\n          <Search class=\"icon-search\" />\n          <input class=\"search\" placeholder=\"Search...\" v-model=\"searchQuery\" />\n        </div>\n        <Bell class=\"icon-bell\" />\n      </header>\n\n      <!-- Stats -->\n      <section class=\"stats grid\">\n        <StatCard\n          :icon=\"GraduationCap\"\n          title=\"Students\"\n          :value=\"stats.students\"\n          bg=\"var(--card-students)\"\n        />\n        <StatCard\n          :icon=\"Bookmark\"\n          title=\"Course\"\n          :value=\"stats.courses\"\n          bg=\"var(--card-courses)\"\n        />\n        <StatCard\n          :icon=\"CreditCard\"\n          :title=\"`Payments`\"\n          :value=\"`${stats.paymentsLabel} ${formatMoney(stats.payments)}`\"\n          bg=\"var(--card-payments)\"\n        />\n        <StatCard\n          :icon=\"User\"\n          title=\"Users\"\n          :value=\"stats.users\"\n          :gradient=\"true\"\n        />\n      </section>\n\n      <!-- Table -->\n      <UsersTable :rows=\"filteredUsers\" @edit=\"onEdit\" @delete=\"onDelete\" />\n    </main>\n  </div>\n</template>\n\n<style scoped>\n.layout {\n  display: grid;\n  grid-template-columns: 260px 1fr;\n  min-height: 100vh;\n  background: #fafafa;\n}\n.content {\n  padding: var(--space-4);\n}\n.topbar {\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n  margin-bottom: var(--space-4);\n  gap: 20px;\n}\n.search-wrapper {\n  position: relative;\n}\n.icon-search {\n  position: absolute;\n  top: 50%;\n  left: 12px;\n  transform: translateY(-50%);\n  color: var(--muted);\n  width: 18px;\n  height: 18px;\n}\n.search {\n  width: 280px;\n  height: 40px;\n  border-radius: 20px;\n  border: 1px solid var(--border);\n  padding: 0 14px 0 36px;\n}\n.icon-bell {\n  width: 22px;\n  height: 22px;\n  color: var(--muted);\n  cursor: pointer;\n}\n.stats {\n  grid-template-columns: repeat(4, 1fr);\n  margin-bottom: var(--space-4);\n}\n@media (max-width: 1100px) {\n  .stats {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n@media (max-width: 640px) {\n  .layout {\n    grid-template-columns: 1fr;\n  }\n}\n</style>\n"],"mappings":";;EAkDOA,KAAK,EAAC;AAAQ;;EAGXA,KAAK,EAAC;AAAS;;EAEXA,KAAK,EAAC;AAAQ;;EACfA,KAAK,EAAC;AAAgB;;EAQpBA,KAAK,EAAC;AAAY;;uBAd/BC,mBAAA,CA4CM,OA5CNC,UA4CM,GA3CJC,YAAA,CAAWC,MAAA,cAEXC,mBAAA,CAwCO,QAxCPC,UAwCO,GAvCLC,mBAAA,YAAe,EACfF,mBAAA,CAMS,UANTG,UAMS,GALPH,mBAAA,CAGM,OAHNI,UAGM,GAFJN,YAAA,CAA8BC,MAAA;IAAtBJ,KAAK,EAAC;EAAa,I,gBAC3BK,mBAAA,CAAsE;IAA/DL,KAAK,EAAC,QAAQ;IAACU,WAAW,EAAC,WAAW;+DAAUN,MAAA,CAAAO,WAAW,GAAAC,MAAA;iDAAXR,MAAA,CAAAO,WAAW,E,KAEpER,YAAA,CAA0BC,MAAA;IAApBJ,KAAK,EAAC;EAAW,G,GAGzBO,mBAAA,WAAc,EACdF,mBAAA,CAyBU,WAzBVQ,UAyBU,GAxBRV,YAAA,CAKEC,MAAA;IAJCU,IAAI,EAAEV,MAAA,CAAAW,aAAa;IACpBC,KAAK,EAAC,UAAU;IACfC,KAAK,EAAEb,MAAA,CAAAc,KAAK,CAACC,QAAQ;IACtBC,EAAE,EAAC;8CAELjB,YAAA,CAKEC,MAAA;IAJCU,IAAI,EAAEV,MAAA,CAAAiB,QAAQ;IACfL,KAAK,EAAC,QAAQ;IACbC,KAAK,EAAEb,MAAA,CAAAc,KAAK,CAACI,OAAO;IACrBF,EAAE,EAAC;8CAELjB,YAAA,CAKEC,MAAA;IAJCU,IAAI,EAAEV,MAAA,CAAAmB,UAAU;IAChBP,KAAK,EAAE,UAAU;IACjBC,KAAK,KAAKb,MAAA,CAAAc,KAAK,CAACM,aAAa,IAAIpB,MAAA,CAAAqB,WAAW,CAACrB,MAAA,CAAAc,KAAK,CAACQ,QAAQ;IAC5DN,EAAE,EAAC;8CAELjB,YAAA,CAKEC,MAAA;IAJCU,IAAI,EAAEV,MAAA,CAAAuB,IAAI;IACXX,KAAK,EAAC,OAAO;IACZC,KAAK,EAAEb,MAAA,CAAAc,KAAK,CAACU,KAAK;IAClBC,QAAQ,EAAE;gDAIftB,mBAAA,WAAc,EACdJ,YAAA,CAAsEC,MAAA;IAAzD0B,IAAI,EAAE1B,MAAA,CAAA2B,aAAa;IAAGC,MAAI,EAAE5B,MAAA,CAAA4B,MAAM;IAAGC,QAAM,EAAE7B,MAAA,CAAA6B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}